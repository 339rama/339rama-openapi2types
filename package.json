{
  "name": "339rama-openapi2types",
  "version": "1.0.0",
  "description": "Utility to convert open api schema to typescript types",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "lint:fix": "eslint \"{**/*,*}.{js,ts}\" --fix",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts}\"",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "keywords": [
    "openapi",
    "convert",
    "typescript",
    "types",
    "cli"
  ],
  "author": "Ramil Bariev (339rama)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "chalk": "^4.1.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "yargs": ">=17",
    "chalk": "=4.1.2"
  },
  "bin": {
    "openapi2types": "./dist/index.js"
  },
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/339rama/339rama-openapi2types.git"
  }
}
